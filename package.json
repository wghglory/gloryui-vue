{
  "name": "dv-components",
  "license": "MIT",
  "author": "Guanghui Wang <guanghui-wang@foxmail.com>",
  "version": "0.0.1",
  "files": [
    "dist"
  ],
  "main": "./dist/dv-components.umd.js",
  "module": "./dist/dv-components.es.js",
  "exports": {
    ".": {
      "import": "./dist/dv-components.es.js",
      "require": "./dist/dv-components.umd.js"
    },
    "./dist/style.css": "./dist/style.css"
  },
  "types": "./dist/types/index.d.ts",
  "scripts": {
    "dev": "vite",
    "build": "vite build && vue-tsc --emitDeclarationOnly && mv dist/src dist/types",
    "preserve": "vite build",
    "serve": "vite preview --port 5050",
    "typecheck": "vue-tsc --noEmit"
  },
  "dependencies": {
    "vue": "^3.2.25"
  },
  "devDependencies": {
    "@types/node": "^17.0.18",
    "@vitejs/plugin-vue": "^2.2.0",
    "autoprefixer": "^10.4.2",
    "postcss": "^8.4.6",
    "tailwindcss": "^3.0.23",
    "typescript": "^4.5.4",
    "vite": "^2.8.0",
    "vue-tsc": "^0.29.8"
  },
  "keywords": [
    "vue component library"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wghglory/dv-components.git"
  }
}
